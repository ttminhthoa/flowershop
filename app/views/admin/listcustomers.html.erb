<div class="title"><span class="title_icon"><img src="/images/bullet5.gif" alt="" /></span>LIST CUSTOMERS</div>
<table border="1" width="300px"  cellpadding="0" cellspacing="0">
	<tr class="form_row">
		<th>Username</th>
	</tr>
<% for customer in @listcustomers %>
 	<tr>
		<td>
			<%=customer.login%>
		</td>
		<td>
			<%=link_to "show",:action => :show,:controller=>:admin,:id => customer %>
		</td>
		<td>
			<%=link_to "edit",:action => :edit,:controller=>:admin,:id => customer %>
		</td>
 	</tr>
<%end%>
</table>
 